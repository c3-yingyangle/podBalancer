{
  "type": "UiSdlConnected<UiSdlFilterPanel>",
  "component": {
    "title": {
      "title": "Filter"
    },
    "settingsMenu": {
      "allowSaveAndLoadConfiguration": false,
      "allowAdvancedFilterMode": false
    },
    "dataSpec": {
      "fieldSets": [
        {
          "fields": [
            {
              "type": "UiSdlFieldSetFilterFieldSetting",
              "fieldName": "statusCategory",
              "label": "Status Category",
              "componentsToFilterMapping": {
                "value": {
                  "PodBalancer.AnalysisTicketsDataGrid": {
                    "RiskFactorDraft": "statusCategory"
                  }
                }
              },
              "filterElement": {
                "type": "UiSdlMultipleSearchFilterField",
                "inputElement": {
                  "clear": true,
                  "fluid": true,
                  "dataSpec": {
                    "dataType": "Issue",
                    "disableDataRequestOnFirstRender": false,
                    "actionName": "evaluate",
                    "args": {
                      "spec": {
                        "projection": "unique(statusCategory)",
                        "filter": "exists(statusCategory) && workstream.id == '${selectedWorkstreamId}'",
                        "limit": -1
                      }
                    },
                    "contextVars": {
                      "selectedWorkstreamId": {
                        "type": "UiSdlApplicationStateValueParam",
                        "id": "PodBalancer.PodBalancerApplicationState",
                        "path": "selectedWorkstream.id"
                      }
                    },
                    "dataPath": "tuples",
                    "valueField": "cells[0].str",
                    "displayField": "cells[0].str"
                  }
                }
              }
            },
            {
              "type": "UiSdlFieldSetFilterFieldSetting",
              "fieldName": "status",
              "label": "Status",
              "componentsToFilterMapping": {
                "value": {
