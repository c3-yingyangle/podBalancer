{
  "type": "UiSdlConnected<UiSdlDataGrid>",
  "component": {
    "wrapWithMetadataId": true,
    "header": {
      "title": "Burndown Per Week"
    },
    "filterBar": false,
    "paginationConfig": {
      "pagination": false
    },
    "removableColumns": true,
    "resizableColumns": true,
    "dataSpec": {
      "dataType": "BurndownUiHelper",
      "advancedDataSpec": {
        "actionName": "getBurndownDataGrid",
        "actionArgs": {
          "workstream": { 
            "id": "${selectedWorkstreamId}"
          }
        }
      },
      "disableContextVarsFilter": true,
      "contextVars": {
        "selectedWorkstreamId": {
          "type": "UiSdlApplicationStateValueParam",
          "id": "PodBalancer.PodBalancerApplicationState",
          "path": "selectedWorkstream.id"
        }
      },
      "columnFields": [
        {
          "fieldName": "id",
          "label": "#",
          "sortable": true,
          "searchable": true
        },
        {
          "fieldName": "date",
          "label": "Date",
          "sortable": true,
          "searchable": true,
          "indicatorMappingField": "status",
          "indicatorMapping": {
            "Past": "#49BFA9", // green
            "Current": "#4F8DFF", // blue
            "Future":  "#A7ADC4" // gray
          }
        },
        {
          "fieldName": "numEngineers",
          "label": "# Engineers",
          "sortable": true,
          "searchable": true
        },
        {
          "fieldName": "numPointsCompletedActual",
          "label": "Pts Completed - Actual",
          "sortable": true,
          "searchable": true
        },
        {
          "fieldName": "numPointsCompletedExpected",
          "label": "Pts Completed - Expected",
          "sortable": true,
          "searchable": true
        },
        {
          "fieldName": "numPointsRemainingActual",
          "label": "Pts Remaining - Actual",
          "sortable": true,
          "searchable": true
        },
        {
          "fieldName": "numPointsRemainingExpected",
          "label": "Pts Remaining - Expected",
          "sortable": true,
          "searchable": true
        },
        {
          "fieldName": "numPointsBehind",
          "label": "Pts Behind",
          "sortable": true,
          "searchable": true,
          "indicatorMappingField": "numPointsBehindStatus",
          "indicatorMapping": {
            "Red": "#F44C48", // red
            "Blue": "#4F8DFF", // blue
            "Green": "#49BFA9" // green
          }
        }
        // {
        //   "fieldName": "pctRemainingActual",
        //   "label": "% Remaining - Actual",
        //   "sortable": true,
        //   "searchable": true
        // },
        // {
        //   "fieldName": "pctRemainingExpected",
        //   "label": "% Remaining - Expected",
        //   "sortable": true,
        //   "searchable": true
        // },
        // {
        //   "fieldName": "changeInScope",
        //   "label": "Change in Scope",
        //   "sortable": true,
        //   "searchable": true
        // }
      ]
    }
  }
}

