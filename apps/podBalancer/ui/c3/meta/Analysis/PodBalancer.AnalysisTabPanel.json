{
  "type": "UiSdlConnected<UiSdlTabPanel>",
  "derivedProps": {
    "title": {
      "text": {
        "type": "UiSdlApplicationStateValueParam",
        "id": "PodBalancer.PodBalancerApplicationState",
        "path": "selectedWorkstream.name"
      }
    }
  },
  "component": {
    "wrapWithMetadataId": true,
    "disableDataRequestOnFirstRender": false,
    "primaryActionButtonTitle": "View on Jira",
    "tabComponents": [
      {
        "id": "PodBalancer.AnalysisBurndownContent",
        "title": "Burndown"
      },
      {
        "id": "PodBalancer.AnalysisEngineersContent",
        "title": "Engineers"
      },
      {
        "id": "PodBalancer.AnalysisHolidaysContent",
        "title": "Holidays"
      }
    ]
  },
  "effectTriggers": [
    {
      "trigger": "PodBalancer.AnalysisTabPanel.ACTION_CLICK_PRIMARY",
      "effectType": "OpenJiraIssuesEpic",
      "payloadStrategy": "MERGE"
    }
  ]
}
